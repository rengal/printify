{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Storage": {
    "MediaRootPath": "c:\\\\media_storage"
  },
  "Listener": {
    "IdleTimeoutSeconds": 2
  },
  "Jwt": {
    "SecretKey": "REPLACE_WITH_32+_CHAR_RANDOM_SECRET",
    "Issuer": "virtual-printer.online",
    "Audience": "Printify.Client",
    "ExpiryHours": 8760
  },
  "Repository": {
    "ConnectionString": "Data Source=printify.db;Cache=Shared"
  },
  "AllowedHosts": "*"
}
